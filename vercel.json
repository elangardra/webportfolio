{
    "version": 2,
    "framework": null,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        { "src": "/build/(.*)", "dest": "/build/$1" },
        { "src": "/assets/(.*)", "dest": "/assets/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "devCommand": "npm run dev"
} 